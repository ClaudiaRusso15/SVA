import turtle 

ninja = turtle.Turtle()

ninja.speed(40)

for i in range(180):
    ninja.forward(5)
    ninja.forward(100)
    ninja.right(80)
    ninja.right(60)
    ninja.forward(10)
    ninja.right(80)
    ninja.forward(80)
    
    
    ninja.penup()
    ninja.setposition(0, 0)
    ninja.pendown()
    
    ninja.right(4)
    
turtle.done()
